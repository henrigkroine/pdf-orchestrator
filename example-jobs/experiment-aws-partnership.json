    "filename": "TEEI-AWS-Partnership-Experiment"
  },

  "export": {
    "pdfPreset": "High Quality Print",
    "formats": ["pdf"]
  },

  "qa": {
    "enabled": true,
    "threshold": 95,
    "auto_fix_colors": true,
    "visualRegression": {
      "enabled": true,
      "baseline": "teei-aws-tfu-v1",
      "maxDiffPercent": 5
    }
  },

  "qaProfile": {
    "id": "aws_tfu_experiment",
    "min_score": 95,
    "min_tfu_score": 140,
    "max_visual_diff_percent": 5,
    "visual_baseline_id": "tfu-aws-v1",
    "create_baseline_on_first_pass": false
  },

  "approval": {
    "mode": "none"
  },

  "mcpFeatures": {
    "useFigmaBrandCheck": false,
    "useAiImages": false,
    "useGitHubSync": false,
    "useNotionSummary": false,
    "useMongoArchive": false
  },

  "mcp": {
    "workflow": "generate-partnership-pdf",
    "servers": {
      "indesign": {
        "enabled": true,
        "actions": ["openTemplate", "applyBrandColors", "bindData", "placeImage", "exportPDF"]
      },
      "playwright": {
        "enabled": true,
        "action": "validateQA"
      }
    }
  },

  "notes": [
    "EXPERIMENT MODE: Tests 3 design variants",
