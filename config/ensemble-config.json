{
  "description": "Multi-Model Ensemble Configuration for AI Vision Validation",
  "version": "3.0.0",
  "lastUpdated": "2025-11-25",
  "models_in_use": ["Claude Opus 4.5", "Gemini Pro 3.0", "GPT-5.1"],
  "tiers": {
    "standard": {
      "name": "Standard Tier",
      "description": "All three models for comprehensive validation",
      "cost": "$0.02 per page",
      "speed": "2-3 seconds per page",
      "accuracy": "98-99%",
      "models": ["claude-opus-4.5", "gemini-pro-3.0", "gpt-5.1"],
      "useCase": "All validation tasks, production workflows, critical documents"
    }
  },
  "models": {
    "claude-opus-4.5": {
      "enabled": true,
      "weight": 0.35,
      "provider": "anthropic",
      "model": "claude-opus-4-5-20251001",
      "strengths": [
        "Best-in-class vision capabilities",
        "Extended thinking for deep reasoning",
        "200K context window",
        "Typography and font analysis",
        "Layout and composition critique",
        "Superior attention to detail"
      ],
      "apiKeyEnv": "ANTHROPIC_API_KEY",
      "costPerImage": 0.015,
      "thinkingEnabled": true,
      "thinkingType": "extended",
      "thinkingBudget": 10000,
      "notes": "Claude Opus 4.5 - Primary model for design critique and brand compliance"
    },
    "gemini-pro-3.0": {
      "enabled": true,
      "weight": 0.35,
      "provider": "google",
      "model": "gemini-3.0-pro",
      "strengths": [
        "Deep reasoning and analysis",
        "2M token context window",
        "Cross-page consistency checking",
        "Superior accuracy",
        "Multi-page document analysis",
        "Advanced visual understanding"
      ],
      "apiKeyEnv": "GEMINI_API_KEY",
      "costPerImage": 0.025,
      "notes": "Gemini Pro 3.0 - Primary model for document-wide analysis and consistency"
    },
    "gpt-5.1": {
      "enabled": true,
      "weight": 0.30,
      "provider": "openai",
      "model": "gpt-5.1",
      "strengths": [
        "Natively multimodal",
        "Superior visual perception",
        "Strong reasoning capabilities",
        "Overall design quality assessment",
        "Professional appearance evaluation",
        "Accessibility considerations"
      ],
      "apiKeyEnv": "OPENAI_API_KEY",
      "costPerImage": 0.015,
      "notes": "GPT-5.1 - Primary model for overall quality assessment and cross-validation"
    }
  },
  "thresholds": {
    "highConfidence": 80,
    "mediumConfidence": 50,
    "lowConfidence": 40,
    "requiresReview": 40,
    "autoAccept": 90,
    "autoReject": 30
  },
  "ensembleStrategy": {
    "votingMethod": "weighted_soft",
    "requireMinimumModels": 2,
    "allowPartialFailure": true,
    "confidenceCalculation": {
      "agreementWeight": 0.6,
      "modelWeightedScoreWeight": 0.4
    }
  },
  "parallelProcessing": {
    "enabled": true,
    "maxConcurrent": 3,
    "timeout": 30000,
    "retryOnFailure": true,
    "maxRetries": 2
  },
  "reporting": {
    "includeIndividualAnalyses": true,
    "includeConfidenceBreakdown": true,
    "includeDisagreementDetails": true,
    "highlightLowConfidenceFindings": true
  }
}
