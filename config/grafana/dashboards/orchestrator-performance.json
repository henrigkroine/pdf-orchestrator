{
  "dashboard": {
    "id": null,
    "uid": "pdf-orchestrator-performance",
    "title": "PDF Orchestrator - Performance",
    "description": "Real-time performance monitoring for PDF Orchestrator",
    "tags": ["pdf-orchestrator", "performance", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Latency Percentiles",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "orchestrator_latency_p50",
            "legendFormat": "P50 (Median)",
            "datasource": "Prometheus"
          },
          {
            "refId": "B",
            "expr": "orchestrator_latency_p95",
            "legendFormat": "P95",
            "datasource": "Prometheus"
          },
          {
            "refId": "C",
            "expr": "orchestrator_latency_p99",
            "legendFormat": "P99",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "axisPlacement": "auto",
              "drawStyle": "line"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "P50 (Median)" },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
            },
            {
              "matcher": { "id": "byName", "options": "P95" },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }]
            },
            {
              "matcher": { "id": "byName", "options": "P99" },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
            }
          ]
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "gauge",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(orchestrator_operations_count[5m])",
            "legendFormat": "Requests/min",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqpm",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 10, "color": "yellow" },
                { "value": 20, "color": "red" }
              ]
            },
            "min": 0,
            "max": 30
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "orchestrator_success_rate",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 95, "color": "yellow" },
                { "value": 98, "color": "green" }
              ]
            },
            "decimals": 2
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 4,
        "title": "Daily Spend",
        "type": "barchart",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "sum(orchestrator_cost_total) by (day)",
            "legendFormat": "Daily Cost",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 20, "color": "yellow" },
                { "value": 25, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "xField": "Time",
          "showValue": "always",
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 5,
        "title": "Monthly Spend Trend",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "expr": "sum(orchestrator_cost_total) by (month)",
            "legendFormat": "Monthly Cost",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "axisPlacement": "auto",
              "drawStyle": "line"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 375, "color": "yellow" },
                { "value": 500, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        }
      },
      {
        "id": 6,
        "title": "Cost Per Document",
        "type": "stat",
        "gridPos": { "x": 0, "y": 16, "w": 8, "h": 6 },
        "targets": [
          {
            "refId": "A",
            "expr": "orchestrator_cost_total / orchestrator_operations_count{operation='generate_document'}",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.75, "color": "yellow" },
                { "value": 1.0, "color": "red" }
              ]
            },
            "decimals": 2
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 7,
        "title": "Circuit Breaker States",
        "type": "state-timeline",
        "gridPos": { "x": 8, "y": 16, "w": 8, "h": 6 },
        "targets": [
          {
            "refId": "A",
            "expr": "orchestrator_circuit_breaker_state",
            "legendFormat": "{{service}}",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "0", "text": "CLOSED", "color": "green" },
              { "type": "value", "value": "1", "text": "HALF_OPEN", "color": "yellow" },
              { "type": "value", "value": "2", "text": "OPEN", "color": "red" }
            ]
          }
        },
        "options": {
          "showValue": "always",
          "rowHeight": 0.9,
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 8,
        "title": "Error Rate by Service",
        "type": "barchart",
        "gridPos": { "x": 16, "y": 16, "w": 8, "h": 6 },
        "targets": [
          {
            "refId": "A",
            "expr": "sum(orchestrator_errors_total) by (service)",
            "legendFormat": "{{service}}",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "xField": "service",
          "showValue": "always",
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  }
}
